{
  "name": "offline-compiler",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "codegen": "./node_modules/.bin/ngc -p ./src",
    "systemjs-builder-bundle": "gulp bundle",
    "rollup": "./node_modules/.bin/rollup -c rollup.js",
    "es5-bundle": "./node_modules/.bin/tsc es6/tree-shaken.js --target es5 --allowJs --outDir es5",
    "minify": "./node_modules/.bin/uglifyjs --screw-ie8 --compress --mangle -- es6/tree-shaken.js > build/build.js",
    "build-with-rollup": "npm run codegen && npm run rollup && node server.js",
    "build-with-systemjs-builder": "npm run codegen && npm run systemjs-builder-bundle && npm run es5-bundle && npm run minify && node server.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "^2.0.0-rc.5",
    "@angular/compiler-cli": "^0.5.0",
    "@angular/core": "2.0.0-rc.5",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "^2.0.0-rc.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babelrc-rollup": "^1.2.0",
    "es6-shim": "0.35.1",
    "express": "^4.14.0",
    "reflect-metadata": "0.1.3",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs-es": "5.0.0-beta.6",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "uglify-js": "2.7.0",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular-cli": "1.0.0-beta.8",
    "codelyzer": "0.0.20",
    "ember-cli-inject-live-reload": "1.4.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-jasmine": "0.3.8",
    "protractor": "3.3.0",
    "ts-node": "0.5.5",
    "tslint": "3.11.0",
    "typescript": "^2.0.0",
    "typings": "0.8.1",
    "gulp": "^3.9.1",
    "systemjs-builder": "^0.15.16",
    "rollup-plugin-node-resolve": "^1.5.0",
    "rollup": "^0.26.1"
  
  }
}
