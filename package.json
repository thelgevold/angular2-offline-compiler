{
  "name": "offline-compiler",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "codegen": "./node_modules/.bin/ngc -p ./src",
    "es6-bundle": "gulp bundle",
    "es5-bundle": "./node_modules/.bin/tsc build/bundle.min.js --target es5 --allowJs --outDir es5",
    "minify": "./node_modules/.bin/uglifyjs --screw-ie8 --compress --mangle -- es5/bundle.min.js > es5/es5.min.js",
    "build": "npm run codegen && npm run es6-bundle && npm run es5-bundle && npm run minify && node server.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/compiler-cli": "^0.4.0",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "^2.0.0-rc.4",
    "es6-shim": "0.35.1",
    "express": "^4.14.0",
    "reflect-metadata": "0.1.3",
    "rxjs-es": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "uglify-js": "2.7.0",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "angular-cli": "1.0.0-beta.8",
    "codelyzer": "0.0.20",
    "ember-cli-inject-live-reload": "1.4.0",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-jasmine": "0.3.8",
    "protractor": "3.3.0",
    "ts-node": "0.5.5",
    "tslint": "3.11.0",
    "typescript": "^1.9.0-dev",
    "typings": "0.8.1",
    "gulp": "^3.9.1",
    "systemjs-builder": "^0.15.16"
  }
}
